<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.lytear.springTest.Dlesson042.dao.DDRealtorDAO"> 
 	<insert id="insertRealtor" parameterType="com.lytear.springTest.Dlesson042.model.DDRealtor"
 	useGeneratedKeys="true" keyProperty="id"> 
 		INSERT INTO `realtor`
 			(
 			`office`
 			,`phoneNumber`
 			,`address`
 			,`grade`
 			,`createdAt`
 			,`updatedAt`
 			
 			
 			)
 		
 		 
 		VALUES ( 
 			#{office } 
 			,#{phoneNumber} 
 			,#{address } 
 			,#{grade } 
 			,now()
 			,now()
 		) 
	</insert> 
	
</mapper>